{
    "DefaultSchema": "dbo",
    "DefaultConnectionString": "PostgreSQL",
    "ConnectionStrings": {
        "PostgreSQL": "Data Source=127.0.0.1;Port=5432;Database=SgdpSample;User Id=postgres;Password=admin;Unicode=true;"
    },
    "Sgdp": {
        "ProductId": "Sgdp.Sample.App",
        "ApplicationId": "Sgdp.Sample",
        "Address": "http://localhost:8080",
        "RabbitMQ": {
            "HostName": "127.0.0.1",
            "VirtualHost": "/",
            "HostPort": 5672,
            "UserName": "guest",
            "Password": "guest"
        },
        "KeepLogsAfterSend": false,
        "ProcessingTimeoutMinutes": 10,
        "ProcessingTimoutCheckDelayMinutes": 2
    },
    "AuthorityEndpoint": "https://{tenant}.rac.dev.totvs.app/totvs.rac",
    "ClientId": "guine_ro",
    "ClientSecret": "totvs@123",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Debug",
                "System": "Error"
            }
        },
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properties": {
            "Application": "Basic Crud"
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "ColoredConsole",
                            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u4}] {SourceContext} {Message}{NewLine}{Exception}"
                        },
                        {
                            "Name": "File",
                            "Args": {
                                "path": "logs/log.txt",
                                "rollingInterval": "Day",
                                "buffered": true,
                                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u4}] {SourceContext} {Message}{NewLine}{Exception}"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
